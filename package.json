{
  "name": "spotify-to-youtube",
  "version": "1.0.0",
  "description": "Converts Spotify playlists to YouTube playlists",
  "main": "lib/js/src/main.js",
  "scripts": {
    "test": "npm run build && (webpack || webpack)",
    "build": "npm run clean && eval $(dependencyEnv) && nopam && bsb -make-world 2>&1 | berror.native --path-to-refmttype $(which refmttype)",
    "no-be": "npm run clean && eval $(dependencyEnv) && nopam && bsb -make-world 2>&1",
    "watch": "npm run clean && eval $(dependencyEnv) && nopam && bsb -make-world -w 2>&1 | berror.native --path-to-refmttype $(which refmttype)",
    "clean": "bsb -clean-world && rm -rf html/js && rm -rf dist",
    "webpack": "webpack -w",
    "postinstall": "electron-rebuild",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryb73/spotify-to-youtube.git"
  },
  "author": "Ryan Biwer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryb73/spotify-to-youtube/issues"
  },
  "homepage": "https://github.com/ryb73/spotify-to-youtube#readme",
  "devDependencies": {
    "bs-fast-csv": "1.0.0",
    "bs-google-apis-web": "1.0.0",
    "bs-node": "github:ryb73/bs-node",
    "bs-node-http-server": "1.0.0",
    "bs-qs": "2.0.0",
    "bs-spotify-web-api": "1.2.0",
    "bs-stream-to-array": "1.0.0",
    "bs-webapi": "0.4.1",
    "copy-webpack-plugin": "4.0.1",
    "dependency-env": "0.1.1",
    "electron": "1.7.2",
    "electron-builder": "19.24.1",
    "electron-rebuild": "1.5.11",
    "fast-csv": "2.4.0",
    "font-awesome": "4.7.0",
    "nopam": "*",
    "ocamlBetterErrors": "*",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "reason-react": "0.1.3",
    "spotify-web-api-node": "2.4.0",
    "string-replace-loader": "1.2.0",
    "webpack": "2.6.1"
  },
  "myreason": "*",
  "dependencies": {
    "bs-platform": "1.7.4",
    "electron-debug": "1.4.0",
    "express": "4.15.4",
    "http-server": "0.10.0",
    "sqlite3": "3.1.8",
    "stream-to-array": "github:ryb73/stream-to-array"
  },
  "build": {
    "appId": "com.electron.spotifyToYoutube",
    "productName": "Spotify to YouTube",
    "directories": {
      "buildResources": "lib/js/src"
    },
    "files": [
      "lib/js/src/**/*",
      "html/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    }
  }
}

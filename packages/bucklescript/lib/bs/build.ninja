ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name spotify-to-youtube
src_root_dir = /Users/ryan/Personal/proj/spotify-to-youtube/packages/bucklescript
bsc = /usr/local/lib/node_modules/bs-platform/bin/bsc.exe
bsdep = /usr/local/lib/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I /Users/ryan/Personal/proj/spotify-to-youtube/packages/bucklescript/node_modules/bs-platform/lib/ocaml -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = 
refmt = /usr/local/lib/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I src
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/main.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/main.re
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/main.mlast.d : build_deps src/main.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/main.cmj $src_root_dir/lib/js/src/main.js src/main.cmi : build_cmj_cmi src/main.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/myNode.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/myNode.re
build src/myNode.mlast.d : build_deps src/myNode.mlast
build src/myNode.cmj $src_root_dir/lib/js/src/myNode.js src/myNode.cmi : build_cmj_cmi src/myNode.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/nodeUrl.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/nodeUrl.re
build src/nodeUrl.mlast.d : build_deps src/nodeUrl.mlast
build src/nodeUrl.cmj $src_root_dir/lib/js/src/nodeUrl.js src/nodeUrl.cmi : build_cmj_cmi src/nodeUrl.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
rule build_ast_and_deps
  command = ${bsc}  ${pp_flags} ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/electron.mlast : build_ast_and_deps $src_root_dir/src/electron.ml
build src/electron.mlast.d : build_deps src/electron.mlast
build src/electron.cmj $src_root_dir/lib/js/src/electron.js src/electron.cmi : build_cmj_cmi src/electron.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build build.ninja : phony  ||  src/electron.mlast.d src/nodeUrl.mlast.d src/myNode.mlast.d src/main.mlast.d
